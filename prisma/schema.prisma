datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           String   @id @default(cuid())
  name         String
  email        String   @unique
  passwordHash String
  hospitalName String?
  createdAt    DateTime @default(now())
  exams        Exam[]
}

model Exam {
  id                 String   @id @default(cuid())
  user               User     @relation(fields: [userId], references: [id])
  userId             String

  uuid               String
  resultadoFinal     String

  detect             Json?
  classify           Json?
  rawJson            Json

  s3Raw              String?
  s3Processed        String?
  s3ResultJson       String?

  imagemTratadaBase64 String?

  createdAt          DateTime @default(now())
}

